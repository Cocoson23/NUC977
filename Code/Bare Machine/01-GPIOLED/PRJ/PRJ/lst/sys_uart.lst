N/**
N *  @brief  UART initialize
N *
N *  @return   0
N */
NINT32 sysInitializeUART(void)
N{
N    /* enable UART0 clock */
N    outpw(REG_CLK_PCLKEN0, inpw(REG_CLK_PCLKEN0) | 0x10000);
X    *((volatile unsigned int *)((0xB0000200+0x18))) = (*((volatile unsigned int *)((0xB0000200+0x18)))) | 0x10000;
N
N    /* GPE0, GPE1 */
N    outpw(REG_SYS_GPE_MFPL, (inpw(REG_SYS_GPE_MFPL) & 0xffffff00) | 0x99);  // UART0 multi-function
X    *((volatile unsigned int *)((0xB0000000+0x090))) = ((*((volatile unsigned int *)((0xB0000000+0x090)))) & 0xffffff00) | 0x99;  
N
N    /* UART0 line configuration for (115200,n,8,1) */
N    outpw(REG_UART0_LCR, inpw(REG_UART0_LCR) | 0x7);
X    *((volatile unsigned int *)((0xB8000000+0x0C))) = (*((volatile unsigned int *)((0xB8000000+0x0C)))) | 0x7;
N    outpw(REG_UART0_BAUD, 0x30000066); /* 12MHz reference clock input, 115200 */
X    *((volatile unsigned int *)((0xB8000000+0x24))) = 0x30000066;  
N    return 0;
N}
