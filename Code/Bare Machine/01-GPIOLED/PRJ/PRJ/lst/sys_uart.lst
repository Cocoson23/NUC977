Nvoid _PutChar_f(UINT8 ch)
N{
N    volatile int loop;
N    while ((inpw(REG_UART0_FSR) & (1<<23))); //waits for TX_FULL bit is clear
X    while (((*((volatile unsigned int *)((0xB8000000+0x18)))) & (1<<23))); 
N    outpw(REG_UART0_THR, ch);
X    *((volatile unsigned int *)((0xB8000000+0x00))) = ch;
N    if(ch == '\n') {
N        while((inpw(REG_UART0_FSR) & (1<<23))); //waits for TX_FULL bit is clear
X        while(((*((volatile unsigned int *)((0xB8000000+0x18)))) & (1<<23))); 
N        outpw(REG_UART0_THR, '\r');
X        *((volatile unsigned int *)((0xB8000000+0x00))) = '\r';
N    }
N}
N
N
Nvoid sysPutString(INT8 *string)
N{
N    while (*string != '\0') {
N        _PutChar_f(*string);
N        string++;
N    }
N}
N
N
Nstatic void sysPutRepChar(INT8 c, INT count)
N{
N    while (count--)
N        _PutChar_f(c);
N}
N
N
Nstatic void sysPutStringReverse(INT8 *s, INT index)
N{
N    while ((index--) > 0)
N        _PutChar_f(s[index]);
N}
N
N
Nstatic void sysPutNumber(INT value, INT radix, INT width, INT8 fill)
N{
N    INT8    buffer[40];
N    INT     bi = 0;
N    UINT32  uvalue;
N    UINT16  digit;
N    UINT16  left = FALSE;
X    UINT16  left = (0);
N    UINT16  negative = FALSE;
X    UINT16  negative = (0);
N
N    if (fill == 0)
N        fill = ' ';
N
N    if (width < 0) {
N        width = -width;
N        left = TRUE;
X        left = (1);
N    }
N
N    if (width < 0 || width > 80)
N        width = 0;
N
N    if (radix < 0) {
N        radix = -radix;
N        if (value < 0) {
N            negative = TRUE;
X            negative = (1);
N            value = -value;
N        }
N    }
N
N    uvalue = value;
N
N    do {
N        if (radix != 16) {
N            digit = uvalue % radix;
N            uvalue = uvalue / radix;
N        } else {
N            digit = uvalue & 0xf;
N            uvalue = uvalue >> 4;
N        }
N        buffer[bi] = digit + ((digit <= 9) ? '0' : ('A' - 10));
N        bi++;
N
N        if (uvalue != 0) {
N            if ((radix == 10)
N                    && ((bi == 3) || (bi == 7) || (bi == 11) | (bi == 15))) {
N                buffer[bi++] = ',';
N            }
N        }
N    } while (uvalue != 0);
N
N    if (negative) {
N        buffer[bi] = '-';
N        bi += 1;
N    }
N
N    if (width <= bi)
N        sysPutStringReverse(buffer, bi);
N    else {
N        width -= bi;
N        if (!left)
N            sysPutRepChar(fill, width);
N        sysPutStringReverse(buffer, bi);
N        if (left)
N            sysPutRepChar(fill, width);
N    }
N}
N
N
Nstatic INT8 *FormatItem(INT8 *f, INT a)
N{
N    INT8   c;
N    INT    fieldwidth = 0;
N    INT    leftjust = FALSE;
X    INT    leftjust = (0);
N    INT    radix = 0;
N    INT8   fill = ' ';
N
N    if (*f == '0')
N        fill = '0';
N
N    while ((c = *f++) != 0) {
N        if (c >= '0' && c <= '9') {
N            fieldwidth = (fieldwidth * 10) + (c - '0');
N        } else if (c == 'l')
N            continue;
N        else
N            switch (c) {
N            case '\000':
N                return (--f);
N            case '%':
N                _PutChar_f('%');
N                return (f);
N            case '-':
N                leftjust = TRUE;
X                leftjust = (1);
N                break;
N            case 'c': {
N                if (leftjust)
N                    _PutChar_f(a & 0x7f);
N
N                if (fieldwidth > 0)
N                    sysPutRepChar(fill, fieldwidth - 1);
N
N                if (!leftjust)
N                    _PutChar_f(a & 0x7f);
N                return (f);
N            }
N            case 's': {
N                if (leftjust)
N                    sysPutString((PINT8)a);
N
N                if (fieldwidth > strlen((PINT8)a))
N                    sysPutRepChar(fill, fieldwidth - strlen((PINT8)a));
N
N                if (!leftjust)
N                    sysPutString((PINT8)a);
N                return (f);
N            }
N            case 'd':
N            case 'i':
N                radix = -10;
N                break;
N            case 'u':
N                radix = 10;
N                break;
N            case 'x':
N                radix = 16;
N                break;
N            case 'X':
N                radix = 16;
N                break;
N            case 'o':
N                radix = 8;
N                break;
N            default:
N                radix = 3;
N                break;      /* unknown switch! */
N            }
N        if (radix)
N            break;
N    }
N
N    if (leftjust)
N        fieldwidth = -fieldwidth;
N
N    sysPutNumber(a, radix, fieldwidth, fill);
N
N    return (f);
N}
