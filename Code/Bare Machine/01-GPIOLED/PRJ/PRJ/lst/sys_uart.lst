N/**
N *  @brief  system UART printf
N *
N *  @param[in]  pcStr    output string
N *
N *  @return   None
N */
Nvoid sysprintf(PINT8 pcStr,...)
N{
N    INT8  *argP;
N
N    vaStart(argP, pcStr);       /* point at the end of the format string */
X    argP = (INT8*)((INT)&pcStr + sizeof(pcStr));        
N    while (*pcStr) {
N        /* this works because args are all ints */
N        if (*pcStr == '%')
N            pcStr = FormatItem(pcStr + 1, vaArg(argP, INT));
X            pcStr = FormatItem(pcStr + 1, ((INT *)(argP += sizeof(INT)))[-1]);
N        else
N            _PutChar_f(*pcStr++);
N    }
N}
