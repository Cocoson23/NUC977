N        for(i=0; i<0x1000; i++);
N        if (!(inpw(REG_UART0_FSR) & (1 << 14))) {
X        if (!((*((volatile unsigned int *)((0xB8000000+0x18)))) & (1 << 14))) {
N            return (inpw(REG_UART0_RBR));
X            return ((*((volatile unsigned int *)((0xB8000000+0x00)))));
N        }
