INT32 GPIO_EnableTriggerType(GPIO_PORT port, UINT32 bitMap, GPIO_TRIGGER_TYPE triggerType)
{
    UINT32 reg;
    INT32 offset;

    offset = (INT32)port;

    switch(triggerType) {
    case LOW:
        reg = (*((volatile unsigned int *)((0xB8003000+0x00C)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x00C)+offset)) = reg | bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x010)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x010)+offset)) = reg & ~bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x014)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x014)+offset)) = reg | bitMap;
        break;
    case HIGH:
        reg = (*((volatile unsigned int *)((0xB8003000+0x00C)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x00C)+offset)) = reg | bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x010)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x010)+offset)) = reg | bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x014)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x014)+offset)) = reg & ~bitMap;
        break;
    case FALLING:
        reg = (*((volatile unsigned int *)((0xB8003000+0x00C)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x00C)+offset)) = reg & ~bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x010)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x010)+offset)) = reg & ~bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x014)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x014)+offset)) = reg | bitMap;
        break;
    case RISING:
        reg = (*((volatile unsigned int *)((0xB8003000+0x00C)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x00C)+offset)) = reg & ~bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x010)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x010)+offset)) = reg | bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x014)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x014)+offset)) = reg & ~bitMap;
        break;
    case BOTH_EDGE:
        reg = (*((volatile unsigned int *)((0xB8003000+0x00C)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x00C)+offset)) = reg & ~bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x010)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x010)+offset)) = reg | bitMap;

        reg = (*((volatile unsigned int *)((0xB8003000+0x014)+offset)));
        *((volatile unsigned int *)((0xB8003000+0x014)+offset)) = reg | bitMap;
        break;
    }
    return 0;
}











 
